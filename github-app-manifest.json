{
  "name": "duyetbot",
  "url": "https://github.com/duyet/duyetbot-agent",
  "hook_attributes": {
    "url": "https://api.duyetbot.dev/github/webhook"
  },
  "redirect_url": "https://duyetbot.dev/auth/github/callback",
  "callback_urls": [
    "https://duyetbot.dev/auth/github/callback",
    "https://api.duyetbot.dev/auth/github/callback"
  ],
  "setup_url": "https://duyetbot.dev/setup",
  "description": "Autonomous AI agent that helps with code reviews, bug analysis, and automated workflows on GitHub",
  "public": true,
  "default_permissions": {
    "issues": "write",
    "pull_requests": "write",
    "contents": "read",
    "metadata": "read"
  },
  "default_events": [
    "issues",
    "issue_comment",
    "pull_request",
    "pull_request_review",
    "pull_request_review_comment"
  ],
  "request_oauth_on_install": true
}
