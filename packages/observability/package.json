    "test:watch": "vitest"
  },
  "keywords": [
    "observability",
    "logging",
    "metrics",
    "d1",
    "cloudflare"
  ],
  "author": "duyet",
  "license": "MIT",
  "dependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "hono": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
