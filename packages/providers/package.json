{
  "name": "@duyetbot/providers",
  "version": "0.1.0",
  "description": "LLM provider implementations for duyetbot-agent",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./claude": {
      "types": "./dist/claude.d.ts",
      "import": "./dist/claude.js"
    },
    "./openrouter": {
      "types": "./dist/openrouter.d.ts",
      "import": "./dist/openrouter.js"
    },
    "./factory": {
      "types": "./dist/factory.d.ts",
      "import": "./dist/factory.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest"
  },
  "keywords": ["llm", "provider", "claude", "openai", "openrouter", "duyetbot"],
  "author": "duyet",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.69.0",
    "@duyetbot/types": "workspace:*",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
