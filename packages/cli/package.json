{
  "name": "@duyetbot/cli",
  "version": "0.1.0",
  "description": "CLI tool for duyetbot agent",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "duyetbot": "./dist/bin.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "echo 'build disabled'",
    "dev": "tsx src/bin.ts",
    "start": "node dist/bin.js",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@duyetbot/core": "workspace:*",
    "@duyetbot/providers": "workspace:*",
    "@duyetbot/tools": "workspace:*",
    "@duyetbot/types": "workspace:*",
    "commander": "^14.0.2",
    "ink": "^6.5.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "react": "^19.0.0"
  },
  "devDependencies": {
    "@types/node": "22.10.0",
    "@types/react": "catalog:",
    "tsx": "^4.20.6",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
