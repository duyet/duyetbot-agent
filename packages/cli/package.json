{
  "name": "@duyetbot/cli",
  "version": "0.1.0",
  "description": "CLI tool for duyetbot agent",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "duyetbot": "./dist/bin.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/bin.ts",
    "start": "node dist/bin.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@duyetbot/core": "workspace:*",
    "@duyetbot/providers": "workspace:*",
    "@duyetbot/tools": "workspace:*",
    "@duyetbot/types": "workspace:*",
    "commander": "^12.1.0",
    "conf": "^13.0.0",
    "ink": "^5.0.1",
    "ink-spinner": "^5.0.0",
    "open": "^10.1.0",
    "ora": "^8.1.0",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "@types/react": "^18.3.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  }
}
