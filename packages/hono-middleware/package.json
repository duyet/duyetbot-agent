{
  "name": "@duyetbot/hono-middleware",
  "version": "1.0.0",
  "description": "Shared Hono middleware and routes for Cloudflare Workers",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run --passWithNoTests",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "hono": "catalog:"
  },
  "devDependencies": {
    "@duyetbot/config-vitest": "workspace:*",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
