# Telegram Bot Configuration
# Copy this file to .env.local and fill in your values
# Then run: source .env.local

# Required
export TELEGRAM_BOT_TOKEN=your_bot_token_from_botfather
export TELEGRAM_WEBHOOK_URL=https://duyetbot-telegram.your-subdomain.workers.dev/webhook
export AI_GATEWAY_URL=https://gateway.ai.cloudflare.com/v1/your-account-id/your-gateway/openrouter/chat/completions

# Optional
export TELEGRAM_WEBHOOK_SECRET=your_webhook_secret
export MODEL=x-ai/grok-4.1-fast
export ALLOWED_USERS=123456789,987654321
