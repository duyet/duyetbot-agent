{
	"name": "@duyetbot/web",
	"version": "3.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --port 3000",
		"check": "biome check . && tsc --noEmit",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"type-check": "tsc --noEmit",
		"dev:worker": "wrangler dev",
		"build": "next build",
		"deploy": "bun run build && wrangler deploy",
		"deploy:branch": "bun run build && wrangler deploy --name duyetbot-web-preview",
		"config": "bun ../../scripts/config.ts web",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "wrangler d1 migrations apply duyetbot --remote",
		"db:studio": "drizzle-kit studio",
		"test": "SESSION_SECRET=test-secret vitest run --passWithNoTests",
		"test:e2e": "playwright test --project=e2e",
		"test:e2e:production": "PLAYWRIGHT_PROJECT=production playwright test --project=production",
		"test:api": "vitest run",
		"test:api:watch": "vitest",
		"test:api:coverage": "vitest run --coverage",
		"test:api:manual": "bun tests/manual/manual-api-test.ts",
		"test:unit": "SESSION_SECRET=test-secret vitest run lib/",
		"test:unit:watch": "SESSION_SECRET=test-secret vitest lib/",
		"test:load": "k6 run tests/load/smoke-test.js",
		"test:load:api": "k6 run tests/load/api-load-test.js",
		"test:load:prod": "k6 run --env URL=https://duyetbot-web.duyet.workers.dev tests/load/smoke-test.js"
	},
	"dependencies": {
