{
  "name": "@duyetbot/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --port 3002",
    "dev:remote": "opennextjs-cloudflare build && opennextjs-cloudflare preview --remote --port 3002",
    "build": "next build",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "deploy:branch": "opennextjs-cloudflare build && opennextjs-cloudflare deploy -- --name duyetbot-web-preview",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "config": "bun ../../scripts/config.ts web",
    "start": "next start",
    "lint": "biome lint ./",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "vitest run --dir src/__tests__/e2e"
  },
  "dependencies": {
    "@ai-sdk/openai-compatible": "^2.0.1",
    "@ai-sdk/react": "^3.0.3",
    "@duyetbot/cloudflare-agent": "workspace:*",
    "@duyetbot/hono-middleware": "workspace:*",
    "@duyetbot/observability": "workspace:*",
    "@duyetbot/prompts": "workspace:*",
    "@duyetbot/providers": "workspace:*",
    "@duyetbot/tools": "workspace:*",
    "@duyetbot/types": "workspace:*",
    "@hono/oauth-providers": "^0.4.0",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "ai": "^6.0.3",
    "arctic": "^3.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "hono": "catalog:",
    "lucide-react": "^0.562.0",
    "motion": "^12.23.26",
    "nanoid": "^5.1.6",
    "next": "^15.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "shiki": "^3.20.0",
    "streamdown": "^1.6.10",
    "tailwind-merge": "^3.4.0",
    "use-stick-to-bottom": "^1.1.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "@opennextjs/cloudflare": "latest",
    "@types/node": "catalog:",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7.2",
    "vitest": "catalog:",
    "wrangler": "catalog:"
  }
}
