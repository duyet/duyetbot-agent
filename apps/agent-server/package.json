{
  "name": "@duyetbot/agent-server",
  "version": "0.1.0",
  "description": "Long-running agent server with filesystem access and Claude Agent SDK",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "dev": "bun --watch src/index.ts",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "start": "bun dist/index.js"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.5.0",
    "@duyetbot/core": "workspace:*",
    "@duyetbot/hono-middleware": "workspace:*",
    "@duyetbot/tools": "workspace:*",
    "@duyetbot/types": "workspace:*",
    "@modelcontextprotocol/sdk": "catalog:",
    "hono": "catalog:",
    "zod": "catalog:",
    "zod-to-json-schema": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "vitest": "^3.2.0"
  }
}
