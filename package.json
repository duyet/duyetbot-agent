{
  "name": "duyetbot-agent",
  "version": "0.1.0",
  "description": "Autonomous bot agent system with multi-LLM support - Monorepo",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "type-check": "turbo run type-check",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "check": "biome check --write . && turbo run type-check",
    "clean": "turbo run clean && rm -rf node_modules",
    "prepare-hooks": "bash scripts/install-hooks.sh",
    "prepare": "bash scripts/install-hooks.sh || true"
  },
  "keywords": [
    "agent",
    "bot",
    "cloudflare-workers",
    "llm",
    "claude",
    "openai",
    "automation",
    "scheduler"
  ],
  "author": "duyet",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.49",
    "@anthropic-ai/sdk": "^0.70.1",
    "@cloudflare/workers-types": "^4.20251121.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "hono": "^4.10.6",
    "ink": "^6.5.0",
    "jose": "^6.1.2",
    "ora": "^9.0.0",
    "react": "^19.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@cloudflare/vitest-pool-workers": "^0.5.18",
    "@hono/node-server": "^1.19.6",
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.6",
    "tsx": "^4.20.6",
    "turbo": "^2.0.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "wrangler": "^3.94.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@10.22.0"
}
