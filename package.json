{
  "name": "duyetbot-agent",
  "version": "0.1.0",
  "description": "Autonomous bot agent system with multi-LLM support - Monorepo",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "type-check": "turbo run type-check",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "check": "biome check --write . && turbo run type-check",
    "clean": "turbo run clean && rm -rf node_modules .turbo",
    "prepare-hooks": "bash scripts/install-hooks.sh",
    "prepare": "bash scripts/install-hooks.sh || true"
  },
  "keywords": [
    "agent",
    "bot",
    "cloudflare-workers",
    "llm",
    "claude",
    "automation",
    "monorepo"
  ],
  "author": "duyet",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@types/node": "catalog:",
    "turbo": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@10.22.0"
}
