{
  "name": "duyetbot-agent",
  "version": "0.1.0",
  "description": "Autonomous bot agent system with multi-LLM support - Monorepo",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*",
    "apps/*",
    "tests/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:safety-kernel": "turbo run build --filter=@duyetbot/safety-kernel",
    "build:shared-agents": "turbo run build --filter=@duyetbot/shared-agents",
    "build:telegram": "turbo run build --filter=@duyetbot/telegram-bot...",
    "build:github": "turbo run build --filter=@duyetbot/github-bot...",
    "build:memory-mcp": "turbo run build --filter=@duyetbot/memory-mcp...",
    "build:dashboard": "turbo run build --filter=@duyetbot/dashboard...",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "type-check": "turbo run type-check",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "check": "biome check --write . && turbo run type-check",
    "clean": "turbo run clean && rm -rf node_modules .turbo",
    "deploy": "turbo run deploy config",
    "deploy:safety-kernel": "turbo run deploy config --filter=@duyetbot/safety-kernel",
    "deploy:shared-agents": "turbo run deploy config --filter=@duyetbot/shared-agents",
    "deploy:telegram": "turbo run deploy config --filter=@duyetbot/telegram-bot...",
    "deploy:github": "turbo run deploy config --filter=@duyetbot/github-bot...",
    "deploy:memory-mcp": "bun run --filter @duyetbot/memory-mcp deploy && bun run config:memory-mcp",
    "deploy:dashboard": "turbo run deploy --filter=@duyetbot/dashboard...",
    "deploy:shared-agents:branch": "turbo run deploy:branch config --filter=@duyetbot/shared-agents",
    "deploy:telegram:branch": "turbo run deploy:branch config --filter=@duyetbot/telegram-bot...",
    "deploy:github:branch": "turbo run deploy:branch config --filter=@duyetbot/github-bot...",
    "deploy:memory-mcp:branch": "bun run --filter @duyetbot/memory-mcp deploy:branch && bun run config:memory-mcp",
    "deploy:safety-kernel:branch": "turbo run deploy:branch config --filter=@duyetbot/safety-kernel",
    "deploy:dashboard:branch": "turbo run deploy:branch --filter=@duyetbot/dashboard...",
    "ci:build:shared-agents": "turbo run build --filter=@duyetbot/shared-agents...",
    "ci:build:telegram": "turbo run build --filter=@duyetbot/telegram-bot...",
    "ci:build:github": "turbo run build --filter=@duyetbot/github-bot...",
    "ci:build:memory-mcp": "turbo run build --filter=@duyetbot/memory-mcp...",
    "ci:build:safety-kernel": "turbo run build --filter=@duyetbot/safety-kernel...",
    "ci:build:dashboard": "turbo run build --filter=@duyetbot/dashboard...",
    "ci:deploy:shared-agents": "bun run --filter @duyetbot/shared-agents deploy",
    "ci:deploy:telegram": "bun run --filter @duyetbot/telegram-bot deploy",
    "ci:deploy:github": "bun run --filter @duyetbot/github-bot deploy",
    "ci:deploy:memory-mcp": "bun run --filter @duyetbot/memory-mcp deploy",
    "ci:deploy:safety-kernel": "bun run --filter @duyetbot/safety-kernel deploy",
    "ci:deploy:dashboard": "bun run --filter @duyetbot/dashboard deploy",
    "ci:deploy-version:shared-agents": "bun run --filter @duyetbot/shared-agents deploy:branch",
    "ci:deploy-version:telegram": "bun run --filter @duyetbot/telegram-bot deploy:branch",
    "ci:deploy-version:github": "bun run --filter @duyetbot/github-bot deploy:branch",
    "ci:deploy-version:memory-mcp": "bun run --filter @duyetbot/memory-mcp deploy:branch",
    "ci:deploy-version:safety-kernel": "bun run --filter @duyetbot/safety-kernel deploy:branch",
    "ci:deploy-version:dashboard": "bun run --filter @duyetbot/dashboard deploy:branch",
    "config": "concurrently 'bun run config:telegram' 'bun run config:github' 'bun run config:shared-agents'",
    "config:telegram": "bun scripts/config.ts telegram",
    "config:github": "bun scripts/config.ts github",
    "config:shared-agents": "bun scripts/config.ts agents",
    "config:memory-mcp": "bun scripts/config.ts memory",
    "config:safety-kernel": "bun scripts/config.ts safety-kernel",
    "config:show": "bun scripts/config.ts show",
    "d1:list": "wrangler d1 list",
    "d1:query": "wrangler d1 execute duyetbot --remote --command",
    "d1:query:memory": "wrangler d1 execute duyetbot-memory --remote --command",
    "d1:shell": "wrangler d1 execute duyetbot --remote",
    "d1:shell:memory": "wrangler d1 execute duyetbot-memory --remote",
    "d1:tables": "wrangler d1 execute duyetbot --remote --command \"SELECT name FROM sqlite_master WHERE type='table'\"",
    "d1:tables:memory": "wrangler d1 execute duyetbot-memory --remote --command \"SELECT name FROM sqlite_master WHERE type='table'\"",
    "prompt:eval": "bun run prompts-eval/scripts/run-all.ts",
    "prompt:eval:router": "cd prompts-eval && promptfoo eval -c configs/router.promptfoo.yaml",
    "prompt:eval:telegram": "cd prompts-eval && promptfoo eval -c configs/telegram.promptfoo.yaml",
    "prompt:eval:github": "cd prompts-eval && promptfoo eval -c configs/github.promptfoo.yaml",
    "prompt:eval:quality": "cd prompts-eval && promptfoo eval -c configs/quality.promptfoo.yaml",
    "prompt:view": "cd prompts-eval && promptfoo view",
    "prompt:share": "cd prompts-eval && promptfoo share",
    "prompt:report": "bun run prompts-eval/scripts/generate-report.ts"
  },
  "keywords": [
    "agent",
    "bot",
    "cloudflare-workers",
    "llm",
    "claude",
    "automation",
    "monorepo"
  ],
  "author": "duyet",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.1.2",
    "@vitest/coverage-v8": "^2.1.8",
    "concurrently": "^9.2.1",
    "promptfoo": "^0.120.1",
    "turbo": "^2.0.0",
    "vitest": "catalog:",
    "wrangler": "^4.53.0"
  },
  "packageManager": "bun@1.3.3",
  "trustedDependencies": [
    "@biomejs/biome",
    "esbuild",
    "sharp",
    "workerd"
  ],
  "catalog": {
    "@modelcontextprotocol/sdk": "^1.24.2",
    "@types/node": "^22.10.0",
    "@types/react": "^19.0.0",
    "agents": "^0.2.24",
    "hono": "^4.10.6",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "wrangler": "^4.50.0",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.24.5"
  },
  "dependencies": {
    "@openrouter/sdk": "^0.1.27"
  }
}
